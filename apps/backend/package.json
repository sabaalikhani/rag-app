{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "pnpm gen:supabase:types && tsc --declaration --outDir dist/",
    "start": "tsx --experimental-wasm-modules -r dotenv/config src/index.ts dotenv_config_path=.env.local",
    "start:dist": "pnpm build && node -r dotenv/config dist/index.js",
    "lint": "eslint src",
    "lint:fix": "pnpm lint --fix",
    "precommit": "lint-staged",
    "format": "prettier --write \"**/*.ts\"",
    "format:check": "prettier --check \"**/*.ts\"",
    "gen:supabase:types": "touch ./src/generated/db.ts && supabase gen types typescript --schema public > ./src/generated/db.ts --project-id <YOUR_PROJECT_ID>"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.41.1",
    "axios": "^1.6.8",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "langchain": "^0.1.30",
    "openai": "^4.31.0",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/pdf-parse": "^1.1.4",
    "@typescript-eslint/parser": "^7.4.0",
    "dotenv": "^16.4.5",
    "esbuild": "^0.20.2",
    "eslint": "^8",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^3.1.0",
    "prettier": "^3.2.5",
    "supabase": "^1.151.1",
    "tsx": "^4.7.1",
    "typescript": "^5"
  }
}
